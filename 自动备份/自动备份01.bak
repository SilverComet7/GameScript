hwnd = dm.FindWindow("地下城与勇士","地下城与勇士") 
dm.MoveWindow hwnd,0,0
dm_ret = dm.BindWindow(hwnd, "normal", "normal", "normal", 0)
// 绑定End

Function 是否洗出思南额外词条()
    has = 0
    hasSiNanXY =  按键找字("司南额外")
    XY = Split(hasSiNanXY,"|")
    If X <> -1 Then
        has = 1// 
    End If
    是否洗出思南额外词条 = has
End Function


Function 洗思南()
    nextSiNan = 是否洗出思南额外词条()
    While nextSiNan = 0
        pos = dm找单个图("C:\Users\chris\Desktop\大漠插件3.1233\A.bmp")
        If int(pos(1)) > 0 Then
            Call dm随机点击(pos(1), pos(2), 30, 30, "R")// 找紫色辟邪玉
            Call dm随机点击(190,580,30,5,"L")     // 点属性融合
            Delay 2500  // 等待属性出现
            nextSiNan = 是否洗出思南额外词条()
        Else 
          洗思南 = 4// 没有玉了结束循环
          nextSiNan = 1
        End If
    Wend
    If nextSiNan = 1 Then 
        Call dm随机点击(600, 35, 30, 15, "L")// 思南界面，去选下一个思南
    End If
End Function


Function 循环洗思南(sx, sy)
    Call dm随机点击(133, 202, 3, 3, "L")// 怀隐
    Call dm随机点击(175,320, 3, 3, "L") //  开启思南界面
    For i = 0 To 4  
        Call dm随机点击(sx + 30 * i, sy, 30, 30, "R")// 选择思南
        当前辟邪玉是否存在额外概率 = 是否洗出思南额外词条()
        If 当前辟邪玉是否存在额外概率 = 0 Then 
            Call dm随机点击(480, 35, 30, 10, "L") // 辟邪玉界面
            i= 洗思南()
        End If
    Next
    dm.KeyPressChar "esc"
End Function


Function 循环删除玉荣(sx, sy)
    Const eachEdge = 30
    For i = 0 to 9
        For j = 0 to 1
            X = sx+eachEdge * i
            Y = sy+eachEdge*j
            Call dm随机点击(X, Y,4,4,"L")
        Next
    Next
    Call dm随机点击(325, 400,5,5,"L") 
    Delay 2000
End Function

Function 清理玉荣()
    Call dm随机点击(133, 202, 3, 3, "L")// 怀隐
    Call dm随机点击(170,255, 3, 3, "L") //  开启分解界面
    Call 循环删除玉荣(490,395) // 前20个
    Call 循环删除玉荣(490, 455)// 后20个
     dm.KeyPressChar "esc"
    清理玉荣 = "清理玉荣结束"
End Function


If Form1.CheckBox1.Value <> 0 Then 
    Call 循环洗思南(480, 105)// 直接洗第一排前5个
End If


If Form1.CheckBox2.Value <> 0 Then
    Call 清理玉荣()
End If



